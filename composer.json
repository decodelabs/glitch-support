{
    "name": "decodelabs/glitch-support",
    "description": "Shared support classes for DecodeLabs\\Glitch",
    "type": "library",
    "keywords": [ "dump", "dumper", "exception", "debug" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "conflict": {
        "decodelabs/veneer": "<0.11.6"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Glitch\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev"
        },
        "phpstan": {
            "includes": [
                "phpstan-extension.neon"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
